%h1 Hello this is the index
-if user_signed_in?
	= current_user.email
	= link_to "Sign Out", destroy_user_session_path, method: :delete
	= link_to "Edit Profil", edit_user_registration_path
-else
	= link_to "Sign In", new_user_session_path

= link_to "Post a website", new_weblink_path

- @weblinks.each do |weblink|
	%h2= link_to weblink.name, weblink
	%p= weblink.description
	%p
		Submited by
		= weblink.user.fullname